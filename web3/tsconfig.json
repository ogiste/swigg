{
  "compilerOptions": {
    "baseUrl": "../",
    "paths": {
      "@modules/*": ["node_modules/*"]
    },
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
,
}
